<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroudgray"
    android:orientation="vertical" >

    <FrameLayout
        android:id="@id/zuchetiaokuan_top_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        >

        <TextView
            android:id="@id/zuchetiaokuan_back"
           style="@style/back" />

        <TextView
           style="@style/topTitle"
            android:text="@string/zuchetiaokuan"
           />
    </FrameLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/hpx1"
        android:background="@color/login_line_gray" />

    <WebView
        android:id="@id/zuchetiaokuan_webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/backgroudgray" />

</LinearLayout>