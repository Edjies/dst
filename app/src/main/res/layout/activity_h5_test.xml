<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_h5_test_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <FrameLayout
        android:id="@+id/activity_h5_test_top_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
    >
    </FrameLayout>
    <FrameLayout
        android:id="@+id/activity_h5_test_content_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <WebView
            android:id="@+id/activity_h5_test_show_web_View"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </WebView>
        <ImageButton
            android:id="@+id/activity_h5_test_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_margin="5dp"
            android:paddingRight="10dp"
            android:paddingTop="5dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="7dp"
            android:src="@drawable/guanbi"
            android:background="@null"
        />
    </FrameLayout>
</LinearLayout>
