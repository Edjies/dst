<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@color/common_white_background_color"
>
    <RelativeLayout
        android:id="@+id/rl_chongidanjilu"
        android:layout_width="match_parent"
        android:layout_height="45dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="18dp"
            android:drawablePadding="13dp"
            android:gravity="center_vertical"
            android:drawableLeft="@drawable/chongdianjilu"
            android:text="充电记录"
            android:textSize="14dp"
            android:textColor="@color/common_item_main_text_color"
        />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:drawableRight="@drawable/right"
            android:layout_marginLeft="18dp"
            android:layout_marginRight="18dp"
            android:text=""
            android:textSize="14dp"
            android:textColor="@color/common_item_sub_text_color"
        />

        <TextView android:layout_width="match_parent" android:layout_height="@dimen/common_divide_height" android:background="@color/common_divide_color" android:layout_alignParentBottom="true"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_shoucangdianzhan"
        android:layout_width="match_parent"
        android:layout_height="45dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="18dp"
            android:drawablePadding="13dp"
            android:gravity="center_vertical"
            android:drawableLeft="@drawable/shoucangdianzhuang"
            android:text="收藏电站"
            android:textSize="14dp"
            android:textColor="@color/common_item_main_text_color"
        />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:drawableRight="@drawable/right"
            android:layout_marginLeft="18dp"
            android:layout_marginRight="18dp"
            android:text=""
            android:textSize="14dp"
            android:textColor="@color/common_item_sub_text_color"
        />

        <TextView android:layout_width="match_parent" android:layout_height="@dimen/common_divide_height" android:background="@color/common_divide_color" android:layout_alignParentBottom="true"/>
    </RelativeLayout>

    <TextView android:layout_width="match_parent" android:layout_height="@dimen/common_divide_tab_height" android:background="@color/common_gray_background_color"/>

    <TextView android:layout_width="match_parent" android:layout_height="@dimen/common_divide_height" android:background="@color/common_divide_color"/>

    <RelativeLayout
        android:id="@+id/rl_gerenziliao"
        android:layout_width="match_parent"
        android:layout_height="45dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="18dp"
            android:drawablePadding="13dp"
            android:gravity="center_vertical"
            android:drawableLeft="@drawable/gerenziliao"
            android:text="个人资料"
            android:textSize="14dp"
            android:textColor="@color/common_item_main_text_color"
        />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:drawableRight="@drawable/right"
            android:layout_marginLeft="18dp"
            android:layout_marginRight="18dp"
            android:text=""
            android:textSize="14dp"
            android:textColor="@color/common_item_sub_text_color"
        />

        <TextView android:layout_width="match_parent" android:layout_height="@dimen/common_divide_height" android:background="@color/common_divide_color" android:layout_alignParentBottom="true"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_huiyuanrenzheng"
        android:layout_width="match_parent"
        android:layout_height="45dp">
        <TextView
            android:id="@+id/tv_huiyuanrenzheng"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="18dp"
            android:drawablePadding="13dp"
            android:gravity="center_vertical"
            android:drawableLeft="@drawable/huiyuanrenzheng"
            android:text="会员认证"
            android:textSize="14dp"
            android:textColor="@color/common_item_main_text_color"
        />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:drawableRight="@drawable/right"
            android:layout_marginLeft="18dp"
            android:layout_marginRight="18dp"
            android:text=""
            android:textSize="14dp"
            android:textColor="@color/common_item_sub_text_color"
        />

        <TextView android:layout_width="match_parent" android:layout_height="@dimen/common_divide_height" android:background="@color/common_divide_color" android:layout_alignParentBottom="true"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_jifenduihuan"
        android:layout_width="match_parent"
        android:layout_height="45dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="18dp"
            android:drawablePadding="13dp"
            android:gravity="center_vertical"
            android:drawableLeft="@drawable/jifenduihuan"
            android:text="积分兑换"
            android:textSize="14dp"
            android:textColor="@color/common_item_main_text_color"
        />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:drawableRight="@drawable/right"
            android:layout_marginLeft="18dp"
            android:layout_marginRight="18dp"
            android:text=""
            android:textSize="14dp"
            android:textColor="@color/common_item_sub_text_color"
        />

        <TextView android:layout_width="match_parent" android:layout_height="@dimen/common_divide_height" android:background="@color/common_divide_color" android:layout_alignParentBottom="true"/>
    </RelativeLayout>



</LinearLayout>